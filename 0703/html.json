"HTML5 Template": {
        "prefix": "html5",
        "body": [
            "<!DOCTYPE html>",
            "<html lang=\"ko\">",
            "<head>",
            "    <meta charset=\"UTF-8\">",
            "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
            "    <title>${1:HTML문서}</title>",
            "</head>",
            "<body>",
            "    $0",
            "</body>",
            "</html>"
        ],
        "description": "Basic HTML5 template"
    }
